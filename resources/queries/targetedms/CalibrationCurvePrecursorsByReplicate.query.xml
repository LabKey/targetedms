<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="CalibrationCurvePrecursorsByReplicate" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="bestRtMean">
                        <columnTitle>RT Mean</columnTitle>
                    </column>
                    <column columnName="lightTotalAreaMean">
                        <columnTitle>Intensity L Mean</columnTitle>
                    </column>
                    <column columnName="heavyTotalAreaMean">
                        <columnTitle>Intensity H Mean</columnTitle>
                    </column>
                    <column columnName="ratioMean">
                        <columnTitle>L/H Ratio Mean</columnTitle>
                    </column>
                    <column columnName="analyteConcentrationMean">
                        <columnTitle>Exp Conc Mean</columnTitle>
                    </column>
                    <column columnName="calculatedConcentrationMean">
                        <columnTitle>Measured Conc Mean</columnTitle>
                    </column>
                    <column columnName="replicateId">
                        <fk>
                            <fkDbSchema>targetedms</fkDbSchema>
                            <fkTable>Replicate</fkTable>
                            <fkColumnName>id</fkColumnName>
                            <fkDisplayColumnName>name</fkDisplayColumnName>
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>